{
  "name": "webclient",
  "version": "2.0.0",
  "license": "UNLICENSED",
  "private": true,
  "author": "Shain Lafazan",
  "scripts": {
    "dev": "nodemon -w server/ server/",
    "setup": "yarn install && yarn setup:compound && yarn setup:kyber",
    "setup:compound": "cd compound && yarn setup && cd ../ && cd kyber && yarn setup",
    "setup:kyber": "cd kyber && yarn setup",
    "build": "next build",
    "start": "NODE_ENV=production node --optimize_for_size --max_old_space_size=920 --gc_interval=100 ./server",
    "ganache": "./node_modules/.bin/ganache-cli --accounts 10 --defaultBalanceEther 1000 --deterministic --mnemonic 'myth like bonus scare over problem client lizard pioneer submit female collect' --networkId 5777 --debug --allowUnlimitedContractSize",
    "migrate": "yarn truffle:migrate:all",
    "truffle:compile": "./node_modules/.bin/truffle compile",
    "truffle:compile:compound": "cd compound && ./node_modules/.bin/truffle compile",
    "truffle:compile:kyber": "cd kyber && ./node_modules/.bin/truffle compile",
    "truffle:compile:all": "yarn truffle:compile:compound && yarn truffle:compile:kyber && yarn truffle:compile",
    "truffle:migrate": "./node_modules/.bin/truffle migrate",
    "truffle:migrate:compound": "cd compound && ./node_modules/.bin/truffle migrate",
    "truffle:migrate:kyber": "cd kyber && ./node_modules/.bin/truffle migrate",
    "truffle:migrate:all": "yarn truffle:migrate:compound && yarn truffle:migrate:kyber && yarn truffle:migrate",
    "truffle:test": "./node_modules/.bin/truffle test",
    "test:unit": "./node_modules/.bin/jest",
    "test:unit:watch": "./node_modules/.bin/jest --watch",
    "test:integration": "./node_modules/.bin/jest --config=./jest.integration.config.js",
    "test:integration:watch": "./node_modules/.bin/jest --config=./jest.integration.config.js --watch",
    "test:watch": "yarn test:unit:watch",
    "test": "yarn test:unit"
  },
  "dependencies": {
    "@material-ui/core": "^4.4.1",
    "@material-ui/icons": "^4.4.1",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "add": "^2.0.6",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "dotenv-webpack": "^1.7.0",
    "express": "^4.17.1",
    "ganache-cli": "^6.7.0",
    "isomorphic-unfetch": "^3.0.0",
    "jest": "^24.9.0",
    "next": "^9.0.5",
    "next-redux-wrapper": "^2.1.0",
    "node-sass": "^4.12.0",
    "prop-types": "^15.6.0",
    "ramda": "^0.26.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "web3": "^1.2.1",
    "ws": "^7.1.2",
    "xmlhttprequest": "^1.8.0",
    "yarn": "^1.17.3"
  },
  "devDependencies": {
    "@mblackmblack/to-seconds": "^0.1.0",
    "bignumber.js": "^9.0.0",
    "eth-token-recover": "^2.3.4",
    "openzeppelin-solidity": "^2.3.0",
    "truffle": "^5.0.41"
  }
}
